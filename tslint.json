{
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "class-name": true,
    "no-var-keyword": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "no-internal-module": true,
    "no-null-keyword": false,
    "prefer-const": true,
    "jsdoc-format": true,
    "interface-name": false,
    "no-console": [
      false,
      "log"
    ],
    "no-var-requires": false,
    "no-any": false
  },
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ]
}